{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Sqlsights",
  "id": "guilhermearpassos-sqlsights-app",
  "backend": true,
  "executable": "gpx_sqlsights",
  "info": {
    "keywords": ["app", "datasource", "panel", "sql"],
    "description": "Complete SQL insights solution with datasource, panels, and app pages",
    "author": {
      "name": "Guilhermearpassos"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "datasource",
      "name": "SQL Sights Data source",
      "path": "nested-datasource/plugin.json"
    },
    {
      "type": "page",
      "name": "Overview",
      "path": "/a/%PLUGIN_ID%/one",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Data Sources",
      "path": "/a/%PLUGIN_ID%/two",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Analytics",
      "path": "/a/%PLUGIN_ID%/three",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Reports",
      "path": "/a/%PLUGIN_ID%/four",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "icon": "cog",
      "name": "Configuration",
      "path": "/plugins/%PLUGIN_ID%",
      "role": "Admin",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.4.0",
    "plugins": []
  }
}