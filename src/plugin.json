{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Sqlsights",
  "id": "guilhermearpassos-sqlsights-app",
  "backend": true,
  "executable": "gpx_sqlsights",
  "info": {
    "keywords": ["app", "datasource", "panel", "sql"],
    "description": "Complete SQL insights solution with datasource, panels, and app pages",
    "author": {
      "name": "Guilhermearpassos"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "chart and snapshots list",
        "path": "docs/img.png"
      },
      {
        "name": "query samples data",
        "path": "docs/img_1.png"
      },
      {
        "name": "query details",
        "path": "docs/img_3.png"
      },
      {
        "name": "query details with warning",
        "path": "docs/img_2.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "datasource",
      "name": "SQL Sights Data source",
      "path": "nested-datasource/plugin.json"
    },
    {
      "type": "page",
      "name": "Overview",
      "path": "/a/%PLUGIN_ID%/one",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "icon": "cog",
      "name": "Configuration",
      "path": "/plugins/%PLUGIN_ID%",
      "role": "Admin",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=12.0.0",
    "plugins": []
  }
}